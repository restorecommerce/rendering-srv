{
  "logger": {
    "elasticsearch": {
      "level": "error",
      "clientOpts": {
        "host": "elasticsearch:9200"
      },
      "source": "rendering-srv"
    }
  },
  "redis": {
    "host": "redis"
  },
  "events": {
    "kafka": {
      "connectionString": "zookeeper:2181"
    }
  },
  "server": {
    "transports": [
      {
        "name": "grpcRendering",
        "provider": "grpc",
        "services": {
          "io-restorecommerce-rendering-srv": "io.restorecommerce.commandinterface.Service"
        },
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "addr": "rendering0srv:50051"
      }
    ]
  }
}
