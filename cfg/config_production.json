{
  "logger": {
    "elasticsearch": {
      "level": "error",
      "clientOpts": {
        "node": "http://elasticsearch:9200"
      },
      "dataStream": true,
      "source": "rendering-srv"
    }
  },
  "redis": {
    "url": "redis://redis:6379"
  },
  "events": {
    "kafka": {
      "kafka": {
        "brokers": [
          "kafka:9092"
        ]
      }
    }
  },
  "server": {
    "transports": [
      {
        "name": "grpcRendering",
        "provider": "grpc",
        "addr": "0.0.0.0:50051"
      }
    ]
  }
}
